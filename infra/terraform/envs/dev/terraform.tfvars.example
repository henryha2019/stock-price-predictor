project_name = "stock-price-predictor"
aws_region   = "us-west-2"

vpc_id = "vpc-xxxxxxxxxxxxxxxxx"

# Two public subnets for ALB/ECS (prefer different AZs)
public_subnet_ids = [
  "subnet-aaaaaaaaaaaaaaaaa",
  "subnet-bbbbbbbbbbbbbbbbb"
]

# One public subnet for the MLflow EC2 instance (pick one of the above)
public_subnet_id = "subnet-aaaaaaaaaaaaaaaaa"

# Your IP with /32
allowed_ssh_cidr    = "YOUR_PUBLIC_IP/32"
allowed_mlflow_cidr = "YOUR_PUBLIC_IP/32"

# Existing EC2 key pair name in this region
mlflow_key_name = "your-keypair-name"

# Optional overrides
mlflow_port        = 5000
api_image_tag      = "latest"
api_desired_count  = 1
api_cpu            = 512
api_memory         = 1024

model_name  = "stock-price-predictor"
model_alias = "prod"
